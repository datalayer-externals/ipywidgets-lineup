{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "lib": [
      "dom",
      "es2015",
      "es2016.array.include",
      "es2017.object"
    ],
    "noEmitOnError": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES6",
    "skipLibCheck": true,
    "sourceMap": true
  }
}
