{
  "name": "lineup_widget",
  "version": "1.0.7",
  "description": "Wrapper around the LineUp.js library for multi attribute rankings",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ipywidgets",
    "widgets"
  ],
  "homepage": "https://github.com/lineupjs/lineup_widget",
  "bugs": {
    "url": "https://github.com/lineupjs/lineup_widget/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "lib/*.css",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/lineupjs/lineup_widget"
  },
  "scripts": {
    "clean": "rimraf dist ../lineup_widget/static lib/*.d.ts lib/embed.js lib/index.js lib/labplugin.js lib/utils.js",
    "precompile": "npm run clean",
    "compile": "tsc --project .",
    "prebuild": "npm run compile",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\"",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@jupyter-widgets/base": ">=2 || 4.0.0-alpha.0",
    "lineupjs": "~4.0.0"
  },
  "devDependencies": {
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
